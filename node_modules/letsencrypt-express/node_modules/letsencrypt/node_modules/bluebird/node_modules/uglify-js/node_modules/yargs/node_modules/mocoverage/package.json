{
  "name": "mocoverage",
  "version": "1.0.0",
  "description": "a mocha reporter with coverage using blanket.",
  "main": "./index.js",
  "scripts": {
    "test": "mocha --check-leaks --ui exports --require blanket -R ../../../"
  },
  "config": {
    "blanket": {
      "pattern": "./index.js",
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "output-reporter": "spec"
    }
  },
